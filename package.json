{
    "name": "node-vote",
    "version": "1.0.0",
    "description": "",
    "main": "app.js",
    "scripts": {
        "start:ts": "cross-env NODE_ENV=locale ts-node -r tsconfig-paths/register src/api/main.ts",
        "start": "cross-env NODE_ENV=locale tsc-watch --onSuccess \"node ./dist/src/api/main.js\"",
        "grpc:ts": "NODE_ENV=locale ts-node -r tsconfig-paths/register src/grpc/main.ts",
        "grpc": "NODE_ENV=locale tsc-watch --onSuccess \"node ./dist/src/grpc/main.js\"",
        "watch-server": "NODE_ENV=locale nodemon --watch 'src/**/*' -e ts --exec 'ts-node' ./src/api/index.js",
        "build": "./build.sh",
        "commitmsg": "npm run lint && validate-commit-msg",
        "lint": "tslint --fix -p tsconfig.json -c tslint.json 'src/**/*.ts'",
        "test": "echo \"Error: no test specified\" && exit 1",
        "jest": "NODE_ENV=locale jest",
        "entity": "rm -rf test/temp & npx typeorm-model-generator -h 127.0.0.1 -d vote -p 3306 -u root -e mysql -x root -o test/temp --noConfig true --ce pascal --cp camel -a true --generateConstructor true"
    },
    "author": "",
    "license": "ISC",
    "config": {
        "commitizen": {
            "path": "./node_modules/cz-conventional-changelog"
        }
    },
