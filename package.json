        "istanbul-harmony": "^0.3.16",
        "mocha": "^3.5.0",
        "nodemon": "^1.19.2",
        "ts-jest": "^24.1.0",
        "ts-node": "^8.4.1",
        "tsc-watch": "^4.0.0",
        "tsconfig-paths": "^3.9.0",
        "tslint": "^5.20.0",
        "typeorm-model-generator": "^0.4.6",
        "typescript": "^3.6.3",
        "validate-commit-msg": "^2.14.0"
    },
    "dependencies": {
        "@nestjs-plus/common": "^1.1.0",
        "@nestjs-plus/rabbitmq": "^1.4.1",
